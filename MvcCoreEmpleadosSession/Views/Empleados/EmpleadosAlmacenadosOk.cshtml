@model List<Empleado>



<h1 style="color:fuchsia">

    Empleados almacenados OK

</h1>



<p>

    <a asp-controller="Empleados"
       asp-action="SessionEmpleadosOk">

        Almacenar empleados

    </a>

</p>



<h2 style="color:red">@ViewData["MENSAJE"]</h2>



@if (Model != null)

{

    <table class="table table-bordered">

        <thead>

            <tr>

                <th>Apellido</th>

                <th>Oficio</th>

                <th>Salario</th>

            </tr>

        </thead>

        <tbody>

            @foreach (Empleado emp in Model)

            {

                <tr>

                    <td>@emp.Apellido</td>

                    <td>@emp.Oficio</td>

                    <td>@emp.Salario</td>

                </tr>

            }

        </tbody>

    </table>

} 